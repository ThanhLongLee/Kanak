
@{
    ViewBag.Title = "Buffalo";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<script src="https://telegram.org/js/telegram-web-app.js"></script>
<script src="https://code.jquery.com/jquery-3.6.0.min.js"></script>
<link rel="stylesheet" type="text/css" href="~/lib/css/buffalo.css">
<script src="~/lib/js/script.js"></script>
<script>
    $(document).ready(function () {
    //let clickCooldown = 1500;
    let clickRecoverRate = 2;
    let clickRecoveryInterval = null;
    let lastClickTime = 0;

        const senderId = Telegram.WebApp.initDataUnsafe.start_param;

        //const urlParams = new URLSearchParams(window.location.search);
        //const senderId = urlParams.get('startapp');
    const user = {
        Id: Telegram.WebApp.initDataUnsafe.user.id,
        Firstname: Telegram.WebApp.initDataUnsafe.user.first_name,
        Lastname: Telegram.WebApp.initDataUnsafe.user.last_name,
        Username: Telegram.WebApp.initDataUnsafe.user.username
        };

        const invites = {
            SenderID: senderId,
            ReceiverID: user.Id,
            ReceiverFirstname: user.Firstname,
            ReceiverLastname: user.Lastname,
            ReceiverUsername: user.Username,
        };    
   
        $.ajax({
            url: '@Url.Action("SendInvites", "Home")',
            type: 'POST',
            contentType: 'application/json',
            data: JSON.stringify({ senderId: senderId, invites: invites }),
            success: function () {
                console.log('Invite sent and user added successfully');
            },
            error: function (xhr, status, error) {
                console.error('Error:', error);
            }
        });

    $.ajax({
        url: '@Url.Action("ProcessUser", "Home")',
        type: 'POST',
        contentType: 'application/json',
        data: JSON.stringify(user),
        success: function (data) {
            $('#username').text(data.Firstname + " " + data.Lastname);
            $('#score').text(data.Score);
            $('#rankName').text(data.RankName);
            $('#rankCount').text(data.CurrentRankPosition + " / " + data.RankCount);

            let clickCooldown = data.click;
            let maxClicks = data.click;
            $('#clicksRemaining').text(clickCooldown);

            const maxPoint = data.MaxPoint;
            const minPoint = data.MinPoint;


            const progress = calculateProgress(data.Score, data.MaxPoint, data.MinPoint);
            $('.progress-gradient').css('width', progress + '%');

            $('#clickButton').on('click', function () {
                const currentTime = new Date().getTime();
                if (clickCooldown > 0) {
                    lastClickTime = currentTime;
                    clickCooldown -= 1;
                    $('#clicksRemaining').text(clickCooldown);

                    const x = event.clientX;
                    const y = event.clientY;

                    const clickSound = document.getElementById('clickSound');
                    clickSound.play();
                    // Tạo phần tử hiển thị điểm cộng
                    $.ajax({
                        url: '@Url.Action("UpdateUserScore", "Home")',
                        type: 'POST',
                        data: JSON.stringify({ userId: user.Id }),
                        contentType: 'application/json',
                        success: function (data) {
                            $('#score').text(data.Score);
                            $('#rankName').text(data.RankName);

                            const updatedProgress = calculateProgress(data.Score, maxPoint, minPoint);
                            $('.progress-gradient').css('width', updatedProgress + '%');

                            const animation = document.createElement('div');
                            animation.className = 'click-animation absolute text-3xl font-bold text-white pointer-events-none';
                            animation.style.position = 'absolute';
                            animation.style.top = `${y}px`;
                            animation.style.left = `${x}px`;
                            animation.style.animation = 'float 1s ease-out';
                            animation.style.zIndex = '9999';
                            animation.textContent = '+' + data.PointAdd;
                            document.body.appendChild(animation);

                            setTimeout(() => {
                                animation.remove();
                            }, 1000);

                        },
                        error: function (xhr, status, error) {
                            console.error('Error:', error);
                        }
                    });
                }

                if (clickCooldown <= 0) {
                    $('#clickButton').prop('disabled', true);
                }
            });

            function startRecovery() {
                if (!clickRecoveryInterval) {
                    clickRecoveryInterval = setInterval(function () {
                        if (new Date().getTime() - lastClickTime > 2000 && clickCooldown < maxClicks) {
                            clickCooldown += clickRecoverRate;
                            if (clickCooldown > maxClicks) {
                                clickCooldown = maxClicks;
                            }
                            $('#clicksRemaining').text(clickCooldown);

                            if (clickCooldown > 0) {
                                $('#clickButton').prop('disabled', false);
                            }
                        }
                    }, 1000);
                }
            }

            startRecovery();
        },
        error: function (xhr, status, error) {
            console.error('Error:', error);
        }
    });

        function calculateProgress(score, maxPoint, minPoint) {
            if (maxPoint === minPoint) return 0; // Tránh chia cho 0
            return ((score - minPoint) / (maxPoint - minPoint)) * 100;
        }
});
</script>

